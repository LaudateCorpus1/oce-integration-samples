{
  "id": "gallerysample",
  "description": "A new VB app",
  "defaultPage": "shell",
  "chains": {
    "resourceChangedHandler": {
      "root": "fireNotification1",
      "description": "Displays a notification message when application has been updated and needs to be refreshed.",
      "variables": {
        "event": {
          "required": false,
          "input": "fromCaller",
          "type": {
            "error": {
              "detail": "string"
            }
          }
        }
      },
      "actions": {
        "fireNotification1": {
          "module": "vb/action/builtin/fireNotificationEventAction",
          "parameters": {
            "summary": "[[ $chain.variables.event.error.detail ]]",
            "target": "leaf"
          }
        }
      }
    }
  },
  "services": {},
  "translations": {
    "app": {
      "path": "./resources/strings/app/nls/app-strings"
    }
  },
  "events": {
    "navigateToItem": {
      "payloadType": {
        "item": "string"
      },
      "description": "Event to configure navigation between items/tabs."
    }
  },
  "types": {},
  "variables": {
    "channelToken": {
      "type": "string",
      "defaultValue": "bb18663017c84bc0a97598d2b086c353"
    },
    "channelToken2": {
      "type": "string",
      "defaultValue": "Your channel token goes here",
      "persisted": "session",
      "@dt": {
        "autowire": {
          "contextKey": "oj-oce-shared",
          "name": "channelToken"
        }
      }
    },
    "contentServer": {
      "type": "string",
      "defaultValue": "https://ocereference-oce0004.cec.ocp.oraclecloud.com"
    },
    "contentServer2": {
      "type": "string",
      "defaultValue": "Your content server goes here",
      "persisted": "session",
      "@dt": {
        "autowire": {
          "contextKey": "oj-oce-shared",
          "name": "contentServer"
        }
      }
    },
    "vbcsServiceId": {
      "type": "string",
      "persisted": "session",
      "@dt": {
        "autowire": {
          "contextKey": "oj-oce-shared",
          "name": "vbcsServiceId"
        }
      }
    }
  },
  "eventListeners": {
    "vbResourceChanged": {
      "chains": [
        {
          "chainId": "resourceChangedHandler",
          "parameters": {
            "event": "{{ $event }}"
          }
        }
      ]
    }
  },
  "userConfig": {
    "type": "vb/DefaultSecurityProvider",
    "configuration": {
      "authentication": {
        "type": "oraclecloud"
      }
   },
    "embedding": "deny"
  },
  "security": {
    "access": {
      "requiresAuthentication": false
    }
  },
  "requirejs": {
    "paths": {
      "oj-oce": "build/components/oj-oce/2.0.2"
    }
  }
}